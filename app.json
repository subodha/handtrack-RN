{
  "expo": {
    "name": "Handtrak-RN",
    "slug": "handtrak-rn",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "This is an experimental project of real-time hand tracking on react native using conventional neural network",
    "githubUrl": "https://github.com/subodha/handtrack-RN"
  }
}
